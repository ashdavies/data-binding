{
  "functions": {
    "predeploy": "./gradlew functions:build",
    "source": "build/js/packages/playground-functions",
    "runtime": "nodejs12"
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "emulators": {
    "functions": {
      "port": 5001
    },
    "firestore": {
      "port": 8080
    },
    "ui": {
      "enabled": true
    }
  }
}
